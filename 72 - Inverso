#include <stdio.h>

int main(){
    int x, N, vetor[1000];
    
    scanf("%d",&N);
    
    for(int i=0; i < N; i++){
        scanf("%d",&x);
        
        vetor[i] = x;
    }
    
    for(int i = 0; i < N; i++){
    	
        if(i < N-1){
            printf("%d ", vetor[N-(i+1)]);
        }else{
            printf("%d\n",vetor[N-(i+1)]);
        }
    }
    
    return 0;    
}
