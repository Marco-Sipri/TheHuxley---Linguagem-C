#include <stdio.h>
#include <string.h>

int main(void) {
    char string[20];
    int c = 0, b = 0;
    
    for(int i = 0; i <= 7; i++) {
       scanf("%[^\n]s", string);
       
       switch(string[0]){
       	case 'b': b++; break;
       	case 'B': b++; break;
       	case 'c': c++; break;
       	case 'C': c++; break;
	   }
    }
    
    if(c > b){
    	printf("CINEMA\n");
	}else{
		printf("BOLICHE\n");
	}
    
}
