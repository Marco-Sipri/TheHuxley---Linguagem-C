#include <stdio.h>

int main() {
    
    int v1[30], v2[30], i, num1, num2;

    scanf("%d", &num1);
    if(num1!=0)
    {
        for(i=0; i<num1; i++)
            scanf("%d", &v1[i]);

        scanf("%d", &num2);
        if(num2==0)
            printf("Erro - A lista deve ter pelo menos 1 elemento.\n");
        else
            for(i=0; i<num1; i++)
                printf("%d ", v1[i]);
            for(i=0; i<num2; i++)
            {
                scanf("%d", &v2[i]);
                printf("%d ", v2[i]);
            }
    }
    else{
        printf("Erro - A lista deve ter pelo menos 1 elemento.\n");
    }
	
	return 0;
}
