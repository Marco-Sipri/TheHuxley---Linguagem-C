#include <stdio.h>
#include <string.h>

int main (){
    char num[15], final[15]; 
	int x, j = 0;

    fgets(num, 15, stdin);
    
    for(int i = 0; i < strlen(num); i++){
    	 switch(num[i]){
    	 	case 'A': num[i] = '2'; break;
    		case 'B': num[i] = '2'; break;
    		case 'C': num[i] = '2'; break;
    		case 'D': num[i] = '3'; break;
    		case 'E': num[i] = '3'; break;
    		case 'F': num[i] = '3'; break;
    		case 'G': num[i] = '4'; break;
    		case 'H': num[i] = '4'; break;
    		case 'I': num[i] = '4'; break;
    		case 'J': num[i] = '5'; break;
    		case 'K': num[i] = '5'; break;
    		case 'L': num[i] = '5'; break;
    		case 'M': num[i] = '6'; break;
    		case 'N': num[i] = '6'; break;
    		case 'O': num[i] = '6'; break;
    		case 'P': num[i] = '7'; break;
    		case 'Q': num[i] = '7'; break;
    		case 'R': num[i] = '7'; break;
    		case 'S': num[i] = '7'; break;
    		case 'T': num[i] = '8'; break;
    		case 'U': num[i] = '8'; break;
    		case 'V': num[i] = '8'; break;
    		case 'W': num[i] = '9'; break;
    		case 'X': num[i] = '9'; break;
    		case 'Y': num[i] = '9'; break;
    		case 'Z': num[i] = '9'; break;
		 }
	}
	
	printf("%s\n", num);
	
    return 0;
}
