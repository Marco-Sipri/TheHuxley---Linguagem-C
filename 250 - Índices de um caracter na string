#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    char palavra[51], letra1;
    int tamanho, i;
    gets(palavra);
    tamanho = strlen(palavra);
    scanf("%c", &letra1); fflush(stdin);
    for(i = 0; i < tamanho; i++)
    {
        if(palavra[i] == letra1)
        {
            printf("%d\n", i);
        }
    }
    printf("-1");
    return 0;
}
